    "def taximetro(distancia, multiplicador=1):\n",
    "    largada = 3\n",
    "    km_rodado = 2\n",
    "    valor = (largada + distancia * \n",
    "    km_rodado) * multiplicador\n",
    "    return valor\n",
    "\n",
    "\n",
    "pagamento = taximetro(3.5)\n",
    "print(pagamento)"
